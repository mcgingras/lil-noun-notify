datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Account {
  id          Int   @id @default(autoincrement())
  discordId   String // unique?
  seeds  SavedSeed[]
}

// implicit many to many

model SavedSeed {
  id         Int @id
  head       Int
  body       Int
  glasses    Int
  accessory  Int
  accounts   Account[]
}
